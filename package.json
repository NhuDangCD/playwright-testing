{
  "name": "playwright-testing-complete",
  "version": "1.0.0",
  "description": "Complete Playwright testing solution with Angular demo application included",
  "author": "NhuDangCD",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NhuDangCD/playwright-testing.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:test": "npm-run-all -p start test:wait",
    "test:wait": "wait-on http://localhost:4200 && npm run test",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "codegen": "playwright codegen",
    "postinstall": "playwright install chromium"
  },
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "12.1.0",
