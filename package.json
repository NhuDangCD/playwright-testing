    "@nebular/security": "10.0.0",
    "@nebular/theme": "10.0.0",
    "@swimlane/ngx-charts": "^20.1.2",
    "angular2-chartjs": "0.4.1",
    "bootstrap": "4.3.1",
    "chart.js": "2.7.1",
    "core-js": "2.5.1",
    "dotenv": "^17.2.1",
    "echarts": "^4.9.0",
    "eva-icons": "^1.1.3",
    "ionicons": "2.0.1",
    "nebular-icons": "1.1.0",
    "ng2-completer": "^9.0.1",
    "ng2-smart-table": "^1.6.0",
    "ngx-echarts": "^4.2.2",
    "normalize.css": "6.0.0",
